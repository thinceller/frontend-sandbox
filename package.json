{
  "name": "frontend-sandbox",
  "version": "1.0.0",
  "description": "This repository is thinceller's Front-end sandbox.",
  "author": "thinceller <thinceller@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "packageManager": "pnpm@7.32.5",
  "engines": {
    "node": "18.15.0",
    "pnpm": "7.32.5",
    "npm": "use pnpm instead",
    "yarn": "use pnpm instead"
  },
  "scripts": {
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0",
    "format": "run-s -c format:*",
    "format:prettier": "prettier --write .",
    "format:eslint": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "devDependencies": {
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.4",
    "prettier-plugin-organize-imports": "3.2.3",
    "typescript": "5.0.4"
  }
}
